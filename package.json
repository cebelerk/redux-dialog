{
  "name": "redux-dialog",
  "version": "0.2.4",
  "description": "A Higher Order Component using react-redux to keep dialog state in a Redux store",
  "main": "dist/redux.dialog.js",
  "module": "dist/redux.dialog.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/suciuvlad/redux-dialog.git"
  },
  "homepage": "https://github.com/suciuvlad/redux-dialog",
  "bugs": "https://github.com/suciuvlad/redux-dialog/issues",
  "scripts": {
    "test": "yarn run jest",
    "build": "NODE_ENV=production rollup -c",
    "docs:dev": "docz dev",
    "prettier": "prettier --config ./.prettierrc --write",
    "prepublish": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "author": "Vlad Suciu",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-modal": "^3.4.5",
    "react-redux": "^5.0.6",
    "redux": "^3.5.2 || ^4.0.0-0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "docz": "^0.12.16",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "husky": "^1.0.0-rc.8",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.6.0",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-modal": "^3.4.5",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.3.0",
    "rollup": "^0.67.4",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.0"
  },
  "tags": [
    "react",
    "redux",
    "react-component",
    "modal",
    "dialog"
  ],
  "keywords": [
    "react",
    "redux",
    "react-component",
    "modal",
    "dialog"
  ],
  "files": [
    "build",
    "src"
  ]
