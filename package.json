{
  "name": "redux-dialog",
  "version": "0.2.1",
  "description": "A Higher Order Component using react-redux to keep dialog state in a Redux store",
  "main": "build/redux.dialog.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/suciuvlad/redux-dialog.git"
  },
  "homepage": "https://github.com/suciuvlad/redux-dialog",
  "bugs": "https://github.com/suciuvlad/redux-dialog/issues",
  "scripts": {
    "test": "yarn run jest",
    "start": "node_modules/.bin/webpack-dev-server --config webpack.config.js --content-base examples/ --progress",
    "build": "NODE_ENV=production webpack --config webpack.production.config.js --progress",
    "prepublish": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "author": "Vlad Suciu",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-modal": "^3.1.5",
    "react-redux": "^5.0.6",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^1.0.0-rc.8",
    "jest": "^21.2.1",
    "prettier": "^1.13.5",
    "pretty-quick": "^1.6.0",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-modal": "^3.1.5",
    "react-redux": "^5.0.6",
    "redux": "^3.5.2",
    "redux-mock-store": "^1.3.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.5"
  },
  "tags": [
    "react",
    "redux",
    "react-component",
    "modal",
    "dialog"
  ],
  "keywords": [
    "react",
    "redux",
    "react-component",
    "modal",
    "dialog"
  ],
  "files": [
    "build",
    "src"
  ]
